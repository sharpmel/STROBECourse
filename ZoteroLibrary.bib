	keywords = {Humans, Female, Male, multiple sclerosis, Adult, Middle Aged, Adolescent, Aged, Age Distribution, Australia, Child, Child, Preschool, Environment, HLA-DR2 Antigen, Infant, New South Wales, Prevalence, Sex Distribution, South Australia},
