	author = {Toews, Ingrid and Binder, Nadine and Wolff, Robert F. and Toprak, Guenes and Elm, Erik von and Meerpohl, Joerg J.},
	month = apr,
	year = {2017},
	keywords = {Research reporting guidelines, Scientific publishing, Bibliometrics, Citation analysis, Geographical regions, Hematology, Longitudinal studies, Oncology},
	pages = {e0176489},
	file = {Full Text PDF:C\:\\Users\\Melissa\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\nwsla9xo.default\\zotero\\storage\\ZMS92H9F\\Toews et al. - 2017 - Guidance in author instructions of hematology and .pdf:application/pdf;Snapshot:C\:\\Users\\Melissa\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\nwsla9xo.default\\zotero\\storage\\HP34RR9Q\\article.html:text/html}
