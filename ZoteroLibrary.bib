	pmid = {12325108},
	keywords = {Humans, Female, Randomized Controlled Trials as Topic, Male, Data Interpretation, Statistical, Publications, Periodicals as Topic, Antihypertensive Agents, Myocardial Infarction, Social Support},
	pages = {2917--2930}
}

