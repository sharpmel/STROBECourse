@article{hawwash_usefulness_2019,
	title = {Usefulness of applying research reporting guidelines as {Writing} {Aid} software: a crossover randomised controlled trial},
	volume = {9},
	copyright = {© Author(s) (or their employer(s)) 2019. Re-use permitted under CC BY-NC. No commercial re-use. See rights and permissions. Published by BMJ.. This is an open access article distributed in accordance with the Creative Commons Attribution Non Commercial (CC BY-NC 4.0) license, which permits others to distribute, remix, adapt, build upon this work non-commercially, and license their derivative works on different terms, provided the original work is properly cited, appropriate credit is given, any changes made indicated, and the use is non-commercial. See: http://creativecommons.org/licenses/by-nc/4.0/.},
	issn = {2044-6055, 2044-6055},
	shorttitle = {Usefulness of applying research reporting guidelines as {Writing} {Aid} software},
	url = {https://bmjopen.bmj.com/content/9/11/e030943},
	doi = {10.1136/bmjopen-2019-030943},
	abstract = {{\textless}h3{\textgreater}Objectives{\textless}/h3{\textgreater} {\textless}p{\textgreater}To assess the intention of using a Writing Aid software, which integrates four research reporting guidelines (Consolidated Standards of Reporting Trials, Preferred Reporting Items for Systematic Reviews and Meta-Analyses, Strengthening the Reporting of Observational Studies in Epidemiology and STrengthening the Reporting of Observational Studies in Epidemiology-nutritional epidemiology) and their Elaboration \&amp; Explanation (E\&amp;E) documents during the write-up of research in Microsoft Word compared with current practices.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Design{\textless}/h3{\textgreater} {\textless}p{\textgreater}Two-arms crossover randomised controlled trial with no blinding and no washout period.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Setting{\textless}/h3{\textgreater} {\textless}p{\textgreater}Face-to-face or online sessions.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Participants{\textless}/h3{\textgreater} {\textless}p{\textgreater}54 (28 in arm 1 and 26 in arm 2) doctoral and postdoctoral researchers.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Interventions{\textless}/h3{\textgreater} {\textless}p{\textgreater}Reporting guidelines and their E\&amp;E document were randomly administered as Writing Aid or as Word documents in a single 30 min to 1 hour session, with a short break before crossing over to the other study intervention.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Primary and secondary outcomes{\textless}/h3{\textgreater} {\textless}p{\textgreater}Using the Technology Acceptance Model, we assessed the primary outcome: the difference in the mean of intention of use; and secondary outcomes: the difference in mean perceived ease of use and perceived usefulness. The three outcomes were measured using questions with a 7-point Likert-scale. Secondary analysis using structural equation modelling (SEM) was applied to explore the relationships between the outcomes.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater} {\textless}p{\textgreater}No significant difference in reported intention of use (mean difference and 95\% CI 0.25 (–0.05 to 0.55), p=0.10), and perceived usefulness (mean difference and 95\% CI 0.19 (–0.04 to 0.41), p=0.10). The Writing Aid performed significantly better than the word document on researchers’ perceived ease of use (mean difference and 95\% CI 0.59 (0.29 to 0.89), p\&lt;0.001). In the SEM analysis, participants’ intention of using the tools was indirectly affected by perceived ease of use (beta 0.53 p\textit{=}0.002).{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusions{\textless}/h3{\textgreater} {\textless}p{\textgreater}Despite no significant difference in the intention of use between the tools, administering reporting guidelines as Writing Aid is perceived as easier to use, offering a possibility to further explore its applicability to enhance reporting adherence.{\textless}/p{\textgreater}},
	language = {en},
	number = {11},
	urldate = {2019-12-03},
	journal = {BMJ Open},
	author = {Hawwash, Dana and Sharp, Melissa K. and Argaw, Alemayehu and Kolsteren, Patrick and Lachat, Carl},
	month = nov,
	year = {2019},
	pmid = {31699728},
	file = {Full Text PDF:C\:\\Users\\Melissa\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\nwsla9xo.default\\zotero\\storage\\LGJ84F56\\Hawwash et al. - 2019 - Usefulness of applying research reporting guidelin.pdf:application/pdf;Snapshot:C\:\\Users\\Melissa\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\nwsla9xo.default\\zotero\\storage\\8I3BWQNK\\e030943.html:text/html}
}

