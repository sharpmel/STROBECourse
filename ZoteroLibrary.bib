@article{irani_weaknesses_2018,
	title = {Weaknesses in the {Reporting} of {Cross}-sectional {Studies} in {Accordance} with the {STROBE} {Report} ({The} {Case} of {Congenital} {Anomaly} among {Infants} in {Iran}): {A} {Review} {Article}},
