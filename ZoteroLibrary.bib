	file = {Snapshot:C\:\\Users\\Melissa\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\nwsla9xo.default\\zotero\\storage\\NST7VRJ7\\abstract.html:text/html;Tomaszewski_et_al-2016-Clinical_Anatomy.pdf:C\:\\Users\\Melissa\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\nwsla9xo.default\\zotero\\storage\\IC45T3HI\\Tomaszewski_et_al-2016-Clinical_Anatomy.pdf:application/pdf}
