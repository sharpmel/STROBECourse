	month = nov,
	year = {2011},
	pmid = {22108262},
	pages = {d6783},
	file = {Cobo et al. - 2011 - Effect of using reporting guidelines during peer r.pdf:C\:\\Users\\Melissa\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\nwsla9xo.default\\zotero\\storage\\WHRT35NQ\\Cobo et al. - 2011 - Effect of using reporting guidelines during peer r.pdf:application/pdf;Snapshot:C\:\\Users\\Melissa\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\nwsla9xo.default\\zotero\\storage\\7KA8M95Q\\bmj.d6783.html:text/html}
