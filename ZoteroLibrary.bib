@article{esene_misclassification_2018,
	title = {Misclassification of {Case} {Control} {Studies} in {Neurosurgery} and {Proposed} {Solutions}},
	issn = {1878-8769},
	doi = {10.1016/j.wneu.2018.01.171},
	abstract = {OBJECT: Case control and cohort studies are common research designs in neurosurgery. But the term "case-control study" (CCS) is frequently misused in the neurosurgical literature with many articles reported as CCS, even though their methodology does not respect the basic components of a CCS. We sought to estimate the extent of these discrepancies in neurosurgical literature, explore factors contributing to mislabeling and shed some light on study design reporting.
METHODS: We identified 31 top-ranking pure neurosurgical journals and searched them for manuscripts reported as CCS, either in the title or the abstract. The manuscripts were read to determine if they really were CCS according to the STROBE guidelines. Manuscript assessment was conducted in duplicate [Agreement (Kappa Statistics) =99.82 \%].
RESULTS: Two hundred and twenty-four articles met our inclusion criteria of which 133 (59.38\%) correctly labeled the case control design whereas 91 articles (40.62\%) misclassified this study design. Cohort studies (CS) were the most common design mislabeled as "case-control" studies in 76 articles (33.93\%), of which 57 articles (25.45\%) were retrospective CS. The mislabeling of CCS impairs the appropriate indexing, classification and sorting of evidence. Mislabeling CS for CCS leads to a downgrading of evidence as CS represent the highest level of evidence for observational studies. Odds ratios instead of relative risk are reported for these studies resulting into a distortion of the measurement of the effect size, compounded when these are summarized in systematic reviews and pooled in meta-analyses.
CONCLUSIONS: A considerable proportion of studies reported as CCS are not true CCS. Reporting guidelines should include items that ensure that studies are labeled correctly. STROBE guidelines be implemented in assessment of observational studies. Researchers in neurosurgery need better training in research methods and terminology. We also recommend accrued vigilance from reviewers and editors.},
	language = {eng},
	journal = {World Neurosurgery},
	author = {Esene, Ignatius Ngene and Mbuagbaw, Lawrence and Dechambenoit, Gilbert and Reda, Wael and Kalangu, Kazadi K.},
	month = feb,
	year = {2018},
	pmid = {29408428},
	keywords = {Case control study, cohort study, neurosurgery, observational studies, research methods}
}

