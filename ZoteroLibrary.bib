@article{knuppel_how_2013,
	title = {How {Psychiatry} {Journals} {Support} the {Unbiased} {Translation} of {Clinical} {Research}. {A} {Cross}-{Sectional} {Study} of {Editorial} {Policies}},
	volume = {8},
	issn = {1932-6203},
	url = {http://journals.plos.org/plosone/article?id=10.1371/journal.pone.0075995},
	doi = {10.1371/journal.pone.0075995},
	abstract = {Introduction Reporting guidelines (e.g. CONSORT) have been developed as tools to improve quality and reduce bias in reporting research findings. Trial registration has been recommended for countering selective publication. The International Committee of Medical Journal Editors (ICMJE) encourages the implementation of reporting guidelines and trial registration as uniform requirements (URM). For the last two decades, however, biased reporting and insufficient registration of clinical trials has been identified in several literature reviews and other investigations. No study has so far investigated the extent to which author instructions in psychiatry journals encourage following reporting guidelines and trial registration. Method Psychiatry Journals were identified from the 2011 Journal Citation Report. Information given in the author instructions and during the submission procedure of all journals was assessed on whether major reporting guidelines, trial registration and the ICMJE’s URM in general were mentioned and adherence recommended. Results We included 123 psychiatry journals (English and German language) in our analysis. A minority recommend or require 1) following the URM (21\%), 2) adherence to reporting guidelines such as CONSORT, PRISMA, STROBE (23\%, 7\%, 4\%), or 3) registration of clinical trials (34\%). The subsample of the top-10 psychiatry journals (ranked by impact factor) provided much better but still improvable rates. For example, 70\% of the top-10 psychiatry journals do not ask for the specific trial registration number. Discussion Under the assumption that better reported and better registered clinical research that does not lack substantial information will improve the understanding, credibility, and unbiased translation of clinical research findings, several stakeholders including readers (physicians, patients), authors, reviewers, and editors might benefit from improved author instructions in psychiatry journals. A first step of improvement would consist in requiring adherence to the broadly accepted reporting guidelines and to trial registration.},
	language = {en},
	number = {10},
	urldate = {2018-03-28},
	journal = {PLOS ONE},
	author = {Knüppel, Hannes and Metz, Courtney and Meerpohl, Joerg J. and Strech, Daniel},
	month = oct,
	year = {2013},
	keywords = {Systematic reviews, Research reporting guidelines, Scientific publishing, Clinical trials, Meta-analysis, observational studies, Clinical trial reporting, Mental health and psychiatry, Observational studies},
	pages = {e75995},
	file = {Full Text PDF:C\:\\Users\\Melissa\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\nwsla9xo.default\\zotero\\storage\\S4D9KQAG\\Knüppel et al. - 2013 - How Psychiatry Journals Support the Unbiased Trans.pdf:application/pdf;Full Text PDF:C\:\\Users\\Melissa\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\nwsla9xo.default\\zotero\\storage\\KCGM27J6\\Knüppel et al. - 2013 - How Psychiatry Journals Support the Unbiased Trans.pdf:application/pdf;Snapshot:C\:\\Users\\Melissa\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\nwsla9xo.default\\zotero\\storage\\KE4TXDXC\\article.html:text/html;Snapshot:C\:\\Users\\Melissa\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\nwsla9xo.default\\zotero\\storage\\HWRNDP9V\\article.html:text/html}
